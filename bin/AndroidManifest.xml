<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yun.wechat"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        <activity
            android:name="com.yun.wechat.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yun.wechat.ChatActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.yun.wechat.PeopleActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.yun.wechat.FindActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.yun.wechat.MyActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>